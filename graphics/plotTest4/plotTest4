#!/usr/bin/gnuplot
#set logscale x 10
set xlabel 'proccessPerNode’
set ylabel 'timesSynced'

# Fijar posición de la leyenda de funciones
set key top left
set title 'Times Synced (1 Nodes)'

#set yrange [0:70]
set term png
set output 'timesSynced1n.png'
plot 'times1n1t.plot' title "EVENTOS" with lines, "times1n2t.plot" title "GRADAS" with lines, "times1n3t.plot" title "PRERES" with lines, "times1n4t.plot" title "ADMIN" with lines, \
	"times1n5t.plot" title "PAGOS" with lines, "times1n6t.plot" title "ANULACIONES" with lines

set title 'Times Synced (2 Nodes)'
set output 'timesSynced2n.png'
plot 'times2n1t.plot' title "EVENTOS" with lines, "times2n2t.plot" title "GRADAS" with lines, "times2n3t.plot" title "PRERES" with lines, "times2n4t.plot" title "ADMIN" with lines, \
	"times2n5t.plot" title "PAGOS" with lines, "times2n6t.plot" title "ANULACIONES" with lines

set title 'Times Synced (3 Nodes)'
set output 'timesSynced3n.png'
plot 'times3n1t.plot' title "EVENTOS" with lines, "times3n2t.plot" title "GRADAS" with lines, "times3n3t.plot" title "PRERES" with lines, "times3n4t.plot" title "ADMIN" with lines, \
	"times3n5t.plot" title "PAGOS" with lines, "times3n6t.plot" title "ANULACIONES" with lines
	
set yrange [0:65]	
set title 'Times Synced (4 Nodes)'
set output 'timesSynced4n.png'
plot 'times4n1t.plot' title "EVENTOS" with lines, "times4n2t.plot" title "GRADAS" with lines, "times4n3t.plot" title "PRERES" with lines, "times4n4t.plot" title "ADMIN" with lines, \
	"times4n5t.plot" title "PAGOS" with lines, "times4n6t.plot" title "ANULACIONES" with lines

set title 'Times Synced (5 Nodes)'
set output 'timesSynced5n.png'
plot 'times5n1t.plot' title "EVENTOS" with lines, "times5n2t.plot" title "GRADAS" with lines, "times5n3t.plot" title "PRERES" with lines, "times5n4t.plot" title "ADMIN" with lines, \
	"times5n5t.plot" title "PAGOS" with lines, "times5n6t.plot" title "ANULACIONES" with lines
