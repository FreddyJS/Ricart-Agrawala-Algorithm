#!/usr/bin/gnuplot
#set logscale x 10
set xlabel 'procesos por nodo'
set ylabel 'tiempo(ms)'

# Fijar posici√≥n de la leyenda de funciones
set key top left
set title 'Tiempo de inercia'

set yrange [5000:5350]
set term png
set output 'tiempoDeInercia.png'
plot 'data/salida1n.plot' title "1 Nodos" with lines, "data/salida2n.plot" title "2 Nodos" with lines, "data/salida3n.plot" title "3 Nodos" with lines, "data/salida4n.plot" title "4 Nodos" with lines, "data/salida5n.plot" title "5 Nodos" with lines
